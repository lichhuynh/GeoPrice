<h1>Listing wishlists</h1>

<table>
  <tr>
    <th>Listid</th>
    <th>Userid</th>
    <th>Lastname</th>
    <th>Lastupdated</th>
    <th>Validfrom</th>
    <th>Validto</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishlists.each do |wishlist| %>
  <tr>
    <td><%= wishlist.ListID %></td>
    <td><%= wishlist.UserID %></td>
    <td><%= wishlist.LastName %></td>
    <td><%= wishlist.LastUpdated %></td>
    <td><%= wishlist.ValidFrom %></td>
    <td><%= wishlist.ValidTo %></td>
    <td><%= link_to 'Show', wishlist %></td>
    <td><%= link_to 'Edit', edit_wishlist_path(wishlist) %></td>
    <td><%= link_to 'Destroy', wishlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wishlist', new_wishlist_path %>
